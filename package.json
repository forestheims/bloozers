{
  "name": "rpt-frontend",
  "version": "1.0.0",
  "description": "frontend template",
  "main": "src/index.jsx",
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch",
    "start": "webpack serve --hot --mode development --devtool eval-source-map --open",
    "start:watch": "npm run start",
    "build": "webpack --devtool source-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forestheims/rpt-frontend.git"
  },
  "keywords": [
    "react"
  ],
  "author": "forestheims",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/forestheims/rpt-frontend/issues"
  },
  "homepage": "https://github.com/forestheims/rpt-frontend#readme",
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "autoprefixer": "^10.4.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.31.8",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "^29.1.1",
    "msw": "^0.47.3",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
